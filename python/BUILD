load("@pybind11_bazel//:build_defs.bzl", "pybind_extension")

# ===========================================================================
# Python extension module (compiled C++)
# ===========================================================================
pybind_extension(
    name = "calculator_py",
    srcs = ["bindings.cpp"],
    deps = ["//lib:Calculator"],
)

# ===========================================================================
# Python package wrapper
# ===========================================================================
py_library(
    name = "calculator",
    srcs = ["__init__.py"],
    data = [":calculator_py.so"],
    visibility = ["//visibility:public"],
)

# ===========================================================================
# Example script (runnable)
# ===========================================================================
py_binary(
    name = "example",
    srcs = ["example.py"],
    deps = [":calculator"],
)load("@pybind11_bazel//:build_defs.bzl", "pybind_extension")

